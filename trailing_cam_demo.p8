--misc
--future position
--position,velocity,acc,frames
function fp(x,v,a,t)
	return x+v*t+0.5*a*t^2
end

--gets acceleration to
--reach goal
function get_a(cx,cvx,ca,camax,
															px,pvx)
	local t=flr(abs((cvx-pvx)/camax))
	local cxf=fp(cx,cvx,-ca,t)
	local pxf=px+pvx*t
	if cxf>pxf then
		ca=-camax
	elseif cxf<pxf then
		ca=camax
	else
		ca=0
	end
	return ca
end

_g=3
--returns a tuple for pull of planets
function gravtuple(x1,y1,m1,x2,y2)
	--calculate distance between center of masses
	local dx=x1-x2
	local dy=y1-y2
	r2=dx^2+dy^2
	--too far away
	if abs(dx)+abs(dy)>200 then return {0,0} end
	if abs(dx)+abs(dy)<1 then return {0,0} end
	if r2<2 then return {1,1} end

	local a2=30/r2--todo mass based constant	
	local angle=atan2(dx,dy)
	--find acceleration 2d-vector
	local ax=cos(angle)*a2
	local ay=sin(angle)*a2
	--if a2!=0 then stop() end
	return {ax,ay}
end

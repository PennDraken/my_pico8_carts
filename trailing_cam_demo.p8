--draw
function _draw()
	cls(0)
	ofs=64
	camera(c.x-ofs,c.y-ofs)
	--camera(p.x-ofs,p.y-ofs)
	map()
	for planet in all(_planets) do
		spr(112,planet.x,planet.y)
	end
	draw_ship()
	rect(c.x-ofs,c.y-ofs,c.x+ofs-1,c.y+ofs-1,1)
end

function draw_ship()
	--draw player
	local angle=atan2(p.vx,p.vy)
	local n=80--start sprite
	if angle>0.875+0.0625 or angle<0.0625 then
		spr(n+0,p.x,p.y)
	elseif angle>0.75+0.0625 then
		spr(n+1,p.x,p.y)
	elseif angle>0.625+0.0625 then
		spr(n+2,p.x,p.y)
	elseif angle>0.5+0.0625 then
		spr(n+3,p.x,p.y)
	elseif angle>0.375+0.0625 then
		spr(n+4,p.x,p.y)
	elseif angle>0.25+0.0625 then
		spr(n+5,p.x,p.y)
	elseif angle>0.125+0.0625 then
		spr(n+6,p.x,p.y)
	else
		spr(n+7,p.x,p.y)
	end
	
	local angle=atan2(p.ax,p.ay)

	local n=96--start sprite
	if angle>0.875+0.0625 or angle<0.0625 then
		spr(n+0,p.x,p.y)
	elseif angle>0.75+0.0625 then
		spr(n+1,p.x,p.y)
	elseif angle>0.625+0.0625 then
		spr(n+2,p.x,p.y)
	elseif angle>0.5+0.0625 then
		spr(n+3,p.x,p.y)
	elseif angle>0.375+0.0625 then
		spr(n+4,p.x,p.y)
	elseif angle>0.25+0.0625 then
		spr(n+5,p.x,p.y)
	elseif angle>0.125+0.0625 then
		spr(n+6,p.x,p.y)
	else
		spr(n+7,p.x,p.y)
	end
end

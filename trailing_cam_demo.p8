--init
function _init()
	--world
	create_planets(30)
	--create_planets(1)
	--add(_planets,new_p(80,80))
	--add(_planets,new_p(100,120))

	--player
	p=new_e(64,64,0.1)
	p.maxv=2
	--camera
	c=new_e(64,64,0.09)
	c.maxv=10
end

function new_e(x,y,amax)
	e={}
	e.x=x--current position
	e.y=y
	e.fx=x--future position
	e.fy=y
	e.vx=0--velocity
	e.vy=0
	e.maxv=100
	e.ax=0--acceleration
	e.ay=0
	e.amax=amax--maximum acc.
	
	return e
end

--planet x,y,radius,mass
function new_p(x,y,r,m)
	p={}
	p.x=x
	p.y=y
	p.r=r
	p.m=m
	return p
end

_planets={}
function create_planets(n)
	for i=0,n do
		add(_planets,new_p(8*flr(rnd(128)),8*flr(rnd(128)),4,10))
	end
end

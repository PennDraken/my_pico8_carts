function _draw()
	cls(7)
	camera(cam.x,cam.y)
	p:draw_trail()
	for fence in all(fences) do 
		fence:draw_shadow()
	end
	for tree in all(trees) do 
		tree:draw_shadow()
	end
	p:draw()
	--map()
	for fence in all(fences) do
		fence:draw()
	end
	for tree in all(trees) do 
		tree:draw()
	end
end

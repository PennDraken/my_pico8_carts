--update
function _update60()
	cam:update(p.x+p.dx*10-64,p.y+p.dy*10-20)

	p:update()
	for fence in all(fences) do
		fence:collides(p)
	end
	for tree in all(trees) do
		tree:collides(p)
	end
	t+=1
end
